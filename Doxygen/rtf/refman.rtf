10 {\cf21 #include "MultimediaManager.h"}\par
11 \par
12 {\cf17 class }ServerTools \{\par
13 {\cf17 public}:\par
14     {\cf20 // Splits the input string 'name' into words and stores them in the array 'parameters'.}\par
15     {\cf20 // Returns the number of words extracted.}\par
16     {\cf20 // If a double space ("  ") is found, returns 1 immediately.}\par
17     {\cf17 static} {\cf18 int} getWords(std::string parameters[], {\cf17 const} std::string& name)\par
18     \{\par
19         {\cf19 if} (name.find({\cf22 "  "}) != std::string::npos) {\cf20 // check for double spaces}\par
20             {\cf19 return} 1;\par
21 \par
22         std::string word; {\cf20 // temporary storage for each word}\par
23         std::stringstream iss(name); {\cf20 // use stringstream to parse words}\par
24         {\cf18 int} i = 0; {\cf20 // word counter}\par
25         {\cf19 while} (iss >> word) {\cf20 // while there are words in the stream}\par
26             parameters[i++] = word; {\cf20 // store each word and increment counter}\par
27         {\cf19 return} i;\par
28     \}\par
29 \par
30     {\cf20 // Processes a protocol request string and interacts with the MultimediaManager.}\par
31     {\cf20 // Returns a response string that is sent back to the client.}\par
32     {\cf17 static} std::string protocolDealer({\cf17 const} std::string& request, MultimediaManager & manager)\par
33 \{\par
34     {\cf17 const} std::string codeEndLine = {\cf22 "\'E7;1"};\par
35     std::ostringstream oss; \par
36 \par
37     {\cf19 if} (request == {\cf22 "lGrp"}) \{  \par
38         manager.listALLGroups(oss);\par
39     \}\par
40     {\cf19 else} {\cf19 if} (request == {\cf22 "lMlt"}) \{\par
41         manager.listAllMultimedias(oss);\par
42     \}\par
43     {\cf19 else} \{\par
44         {\cf20 // Assume the request has a command and an argument.}\par
45         {\cf19 if} (request.length() < 5)\par
46             {\cf19 return} {\cf22 "Invalid request format"} + codeEndLine;\par
47                 \par
48         std::string command = request.substr(0, 4);\par
49         {\cf18 int} stringSize = request.length() - 5; {\cf20 // 4 chars for command and 1 for space}\par
50         std::string name = request.substr(5, stringSize);\par
51 \par
52         {\cf19 if} (command.substr(0, 3) == {\cf22 "add"}) \{\par
53             {\cf18 int} width = 0;\par
54             {\cf19 if} (command == {\cf22 "addV"})\par
55                 width = 3;\par
56             {\cf19 else} {\cf19 if} (command == {\cf22 "addP"})\par
57                 width = 4;\par
58             {\cf19 else}\par
59                 {\cf19 return} {\cf22 "Unknown add command"} + codeEndLine;\par
60 \par
61             std::string parameters[10];  {\cf20 // fixed size array}\par
62             {\cf18 int} count = getWords(parameters, name);\par
63             {\cf19 if} (count < width)\par
64                 {\cf19 return} {\cf22 "Make sure the form is complete before trying again."} + codeEndLine;\par
65 \par
66             {\cf19 try} \{\par
67                 {\cf19 if} (width == 3)\par
68                     manager.createVideo(parameters[0], parameters[1], std::stoi(parameters[2]));\par
69                 {\cf19 else}\par
70                     manager.createPhoto(parameters[0], parameters[1], std::stoi(parameters[2]), std::stoi(parameters[3]));\par
71             \}\par
72             {\cf19 catch} ({\cf17 const} std::invalid_argument &e) \{\par
73                 {\cf19 return} {\cf22 "Bad input: invalid argument"} + codeEndLine;\par
74             \}\par
75             {\cf19 catch} ({\cf17 const} std::out_of_range &e) \{\par
76                 {\cf19 return} {\cf22 "Integer overflow: out of range"} + codeEndLine;\par
77             \}\par
78             {\cf19 return} {\cf22 "adding "} + parameters[0] + codeEndLine;\par
79         \}\par
80         {\cf19 else} \{\par
81             {\cf19 if} (stringSize > 0) \{\par
82                 {\cf19 if} (command == {\cf22 "play"}) \{\par
83                     manager.playMultimedia(name);\par
84                     {\cf19 return} {\cf22 "playing "} + name + codeEndLine;\par
85                 \}\par
86                 {\cf19 else} {\cf19 if} (command == {\cf22 "find"}) \{\par
87                     std::ostringstream ossFind;\par
88                     manager.displayMultimedia(name, ossFind);\par
89                     {\cf19 return} ossFind.str() + codeEndLine;\par
90                 \}\par
91                 {\cf19 else} {\cf19 if} (command == {\cf22 "remv"}) \{\par
92                     manager.removeMultimedia(name);\par
93                     {\cf19 return} {\cf22 "removing "} + name + codeEndLine;\par
94                 \}\par
95                 {\cf19 else} \{\par
96                     {\cf19 return} {\cf22 "command not found"} + codeEndLine;\par
97                 \}\par
98             \}\par
99             {\cf19 else} \{\par
100                 {\cf19 return} {\cf22 "Please, provide a name!"} + codeEndLine;\par
101             \}\par
102         \}\par
103     \}\par
104 \par
105     {\cf20 // Get the complete response from the stream.}\par
106     std::string response = oss.str();\par
107     {\cf20 // Replace newline and carriage return characters with a space (or any character that does not interfere)}\par
108     std::replace(response.begin(), response.end(), {\cf23 '\\n'}, {\cf23 ' '});\par
109     std::replace(response.begin(), response.end(), {\cf23 '\\r'}, {\cf23 ' '});\par
110     {\cf20 // Append your custom end-of-message code.}\par
111     {\cf19 return} response + codeEndLine;\par
112 \}\par
113 \par
114 \};\par
115 \par
116 {\cf21 #endif}\par
}
